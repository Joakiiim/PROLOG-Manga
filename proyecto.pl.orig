manga_tiene_mas_de_10(Manga) :- cantidad_tomos(Manga, Tomos), Tomos >= 10.
manga_tiene_menos_de_20_tomos(Manga) :- cantidad_tomos(Manga, Tomos), Tomos < 20.
manga_tiene_mas_de_20(Manga) :- cantidad_tomos(Manga, Tomos), Tomos >= 20.
